{
  "$schema": "http://json-schema.org/schema",
  "$id": "Library",
  "cli": "nx",
  "title": "Create a node service",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Service name",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use for the service?"
    }
  },
  "required": ["name"],
  "additionalProperties": false
}
